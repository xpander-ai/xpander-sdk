  "devDependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.624.0",
    "@langchain/openai": "^0.2.6",
    "@types/jest": "^27",
    "@types/node": "^22",
    "@typescript-eslint/eslint-plugin": "^7",
    "@typescript-eslint/parser": "^7",
    "constructs": "^10.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^27",
    "jest-junit": "^15",
    "jsii": "~5.7.0",
    "jsii-diff": "^1.101.0",
    "jsii-docgen": "^10.4.17",
    "jsii-pacmak": "^1.101.0",
    "jsii-rosetta": "~5.7.0",
    "openai": "latest",
    "prettier": "^3.3.2",
    "projen": "^0.82.8",
    "standard-version": "^9",
    "ts-jest": "^27",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "sync-request": "^6.1.0"
  },
  "bundledDependencies": [
    "sync-request"
  ],
  "engines": {
    "node": ">= 22.15.0"
  },
  "main": "lib/index.js",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.0",
  "jest": {
    "coverageProvider": "v8",
    "injectGlobals": true,
    "detectOpenHandles": true,
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/@(test|src)/**/*(*.)@(spec|test).ts?(x)"
    ],
    "clearMocks": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "cobertura",
      "text"
    ],
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-reports"
        }
      ]
    ],
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.dev.json"
      }
    }
  },
  "types": "lib/index.d.ts",
  "stability": "stable",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "xpander-sdk",
        "module": "xpander_sdk"
      }
    },
    "tsc": {
      "outDir": "lib",
      "rootDir": "src"
    }
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.ts and run \"npx projen\"."
}
